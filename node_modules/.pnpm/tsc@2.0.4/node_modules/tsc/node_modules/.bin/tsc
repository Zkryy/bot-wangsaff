#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules/tsc/bin/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules/tsc/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules/tsc/bin/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules/tsc/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/tsc@2.0.4/node_modules:/mnt/c/Users/PKL Ibadurrrahman/bot-wa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
